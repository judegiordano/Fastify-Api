    "@types/node": "^14.14.34",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "eslint": "^7.21.0",
    "loadtest": "^5.1.2",
    "ts-node": "^9.1.1",
    "typeorm-model-generator": "^0.4.5",
    "typescript": "^4.2.3"
  },
